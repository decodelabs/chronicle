{
    "name": "decodelabs/chronicle",
    "description": "Release notes generator",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/atlas": "^0.14",
        "decodelabs/dovetail": "^0.3",
        "decodelabs/enumerable": "^0.2.8",
        "decodelabs/exceptional": "^0.6.3",
        "decodelabs/slingshot": "^0.3",
        "decodelabs/stash": "^0.7",
        "decodelabs/systemic": "^0.12",

        "nesbot/carbon": "^3",
        "guzzlehttp/guzzle": "^7",
        "http-interop/http-factory-guzzle": "^1.2",
        "knplabs/github-api": "^3.16",
        "z4kn4fein/php-semver": "^3.0"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Chronicle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    }
}
